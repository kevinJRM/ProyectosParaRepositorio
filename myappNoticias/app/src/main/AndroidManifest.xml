<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/leyendo_el_periodico_en_la_mesa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/leyendo_el_periodico_en_la_mesa"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyappNoticias"
        android:usesCleartextTraffic="true"
        tools:targetApi="27">
        <activity
            android:name=".a_ui.FragmentNoticiasPospuestas"
            android:exported="true"/>
        <activity
            android:name=".a_ui.Noticia"
            android:exported="true"/>
        <activity
            android:name=".a_ui.FragmentNoticiasMain"
            android:exported="true" />


        <activity
            android:name=".a_ui.SplashActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>